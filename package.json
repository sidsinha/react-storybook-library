{
  "name": "react-storybook-library",
  "version": "1.32.0",
  "description": "CONTENT VIEWER",
  "main": "src/index.js",
  "scripts": {
    "test": "jest --verbose --coverage",
    "storybook": "start-storybook -p 6006 .storybook",
    "build-storybook": "build-storybook",
    "rollup": "rollup -c",
    "start": "yarn storybook",
    "transpile": "node_modules/.bin/cross-env NODE_ENV=production node_modules/.bin/babel --source-maps --out-dir ./dist/build/es src --copy-files --ignore *.story.js,*.spec.js",
    "prebuild": "rimraf node_modules/slick-carousel/slick/slick.scss",
    "build:es2015": "node_modules/.bin/cross-env NODE_ENV=production babel --config-file ./.babelrc ./src --out-dir ./dist/build --copy-files --ignore *.stories.js,*.spec.js",
    "build:es2015modules": "node_modules/.bin/cross-env NODE_ENV=production BABEL_ENV=modules babel --config-file ./.babelrc ./src/index.js --out-file ./dist/build/es/index.es.js --copy-files",
    "build:es": "node_modules/.bin/cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ./.babelrc ./src --out-dir ./dist/build/es --source-maps --copy-files --ignore *.stories.js,*.spec.js",
    "build:copy-files": "node ./scripts/copy-files.js --verbose",
    "build": "rimraf dist && yarn build:es2015 && yarn build:es && yarn build:es2015modules && yarn rollup && yarn build:copy-files",
    "github-release": "conventional-github-releaser -p angular",
    "github-storybook": "storybook-to-ghpages --existing-output-dir=./storybook-static --ci --host-token-env-variable=CONVENTIONAL_GITHUB_RELEASER_TOKEN",
    "package-reset": "node ./scripts/reset-package-version.js --verbose",
    "prepublish": "yarn build",
    "release:next": "yarn build && npm publish dist/build --tag next",
    "release": "yarn build && npm publish dist/build",
    "add-locale": "lingui add-locale",
    "extract": "lingui extract",
    "compile": "lingui compile",
    "precommit": "lint-staged",
    "postcommit": "git update-index --again"
  },
  "author": "sid.sinhaa@gmail.com",
  "license": "UNLICENSED",
  "peerDependencies": {
    "material-ui": "^0.18.3",
    "prop-types": "^15.5.4",
    "react": "^0.14.9 || ^15.0.0 || ^16.8.0",
    "react-dom": "^0.14.9 || ^15.0.0 || ^16.8.0"
  },
  "dependencies": {
    "@babel/core": "^7.4.0",
    "@fortawesome/fontawesome-svg-core": "1.2.15",
    "@fortawesome/free-solid-svg-icons": "5.7.2",
    "@fortawesome/pro-light-svg-icons": "5.7.2",
    "@fortawesome/pro-solid-svg-icons": "5.7.2",
    "@fortawesome/react-fontawesome": "0.1.4",
    "@hig/flyout": "0.6.0",
    "@hig/icon": "0.2.1",
    "@hig/icon-button": "0.2.2",
    "@hig/rich-text": "0.1.4",
    "@lingui/core": "^2.7.4",
    "@lingui/macro": "^2.7.4",
    "@lingui/react": "^2.7.4",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.5",
    "draft-js": "^0.10.5",
    "jquery": "^3.3.1",
    "json-query": "^2.2.2",
    "lodash": "^4.17.11",
    "loglevel": "^1.6.1",
    "prop-types": "^15.5.7",
    "react-dotdotdot": "1.2.3",
    "react-html-parser": "2.0.2",
    "react-share": "2.4.0",
    "styled-components": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.0",
    "@lingui/cli": "^2.7.4",
    "@storybook/addon-actions": "^5.0.0",
    "@storybook/addon-knobs": "^5.0.0",
    "@storybook/addon-links": "^5.0.0",
    "@storybook/addon-viewport": "^5.0.0",
    "@storybook/addons": "^5.0.0",
    "@storybook/react": "^5.0.0",
    "@storybook/storybook-deployer": "^2.3.0",
    "@types/enzyme": "^3.9.1",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "axios-mock-adapter": "^1.16.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-plugin-macros": "^2.5.0",
    "child_process": "^1.0.2",
    "classnames": "^2.2.6",
    "commitizen": "^3.0.7",
    "conventional-changelog-cli": "^2.0.5",
    "conventional-github-releaser": "^3.1.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "date-fns": "^1.29.0",
    "edit-json-file": "^1.1.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint-config-google": "^0.12.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-security": "^1.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
    "fs": "^0.0.1-security",
    "fs-extra": "^7.0.0",
    "gh-pages": "^2.0.1",
    "glob": "^7.1.3",
    "interpolate-loader-options-webpack-plugin": "0.0.1",
    "jest": "^24.5.0",
    "jest-dom": "^3.1.3",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.5.0",
    "minimist": "^1.2.0",
    "node-sass": "^7.0.0",
    "path": "^0.12.7",
    "postcss-nested": "^4.1.2",
    "postcss-preset-env": "^6.6.0",
    "postcss-simple-vars": "^5.0.2",
    "qs": "^6.5.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "^2.1.8",
    "react-test-renderer": "^16.8.5",
    "react-testing-library": "^6.0.2",
    "rimraf": "^2.6.2",
    "rollup": "^1.6.0",
    "rollup-plugin-alias": "^1.5.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-peer-deps-external": "^2.0.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-svg": "^1.0.1",
    "sass-loader": "^7.1.0",
    "storybook-addon-versions": "^0.4.3",
    "storybook-readme": "^5.0.0",
    "style-loader": "^0.23.0",
    "svgo": "^1.0.5",
    "svgo-loader": "^2.2.0",
    "url": "^0.11.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/index.js",
      "!**/*.stories.js",
      "!**/node_modules/**",
      "!**/dist/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  },
  "awsConfig": {
    "storybookBucket": "dpe-dhx-react",
    "storybookOutput": "./storybook-static"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
